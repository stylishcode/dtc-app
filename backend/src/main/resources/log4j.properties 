log4j.rootLogger=ERROR,stdout, file
log4j.appender.stdout=org.apache.log4j.ConsoleAppender
log4j.appender.Target=System.out
log4j.appender.layout =org.apache.log4j.PatternLayout
log4j.appender.layout.ConversionPattern=%d(yyyy-MM-dd HH:mm:ss) %-5p %c(1):%L -%m%n

log4j.appender.file=org.apache.log4j.RollingFileAppender
log4j.appender.file.File=dtc-spring.log
log4j.appender.file.MaxFileSize=5MB
log4j.appender.file.MaxBackupIndex=1
log4j.appender.file.layout=org.apache.log4j.PatternLayout
log4j.appender.fileOut.layout.ConversionPattern=%d [%t] %5p %c:%L - %m%n
